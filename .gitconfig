[user]
    name = James Harris
    email = james.harris@icecave.com.au

[core]
    autocrlf = input
    excludesfile = ~/.gitignore.global

[help]
    autocorrect = 30

[color]
    ui = auto

[format]
    pretty = %C(white)%h%C(reset) %C(blue)%cn%C(reset) :%C(green)%d%C(reset) %s %C(blue)(%cr)%C(reset)

[push]
    default = simple
